{
  "name": "mxm",
  "version": "0.8.7",
  "description": "MXM Server",
  "productName": "MXM",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Carlos Rueda <carueda@mbari.org>",
  "license": "Apache-2.0",
  "scripts": {
    "start": "bin/esm.js",
    "mon": "nodemon bin/esm.js",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest",
    "test:unit:updateSnapshot": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "dependencies": {
    "axios": "0.27.2",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "graphile-build-pg": "4.12.3",
    "graphile-utils": "4.12.3",
    "graphql": "14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.21",
    "pg": "8.7.3",
    "postgraphile": "4.12.11",
    "serve-static": "1.15.0"
  },
  "resolutions": {
    "graphql": "14.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.10",
    "babel-jest": "^28.1.3",
    "jest": "28.1.3",
    "majestic": "1.8.1",
    "sass": "^1.54.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
